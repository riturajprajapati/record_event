{
  "manifest_version": 3,
  "name": "Event Record demo 2",
  "version": "1.1.3",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icon48.png"
    }
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": ["scripting","activeTab","contextMenus","management","storage"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "all_frames":true,
      "run_at":"document_start",
      "matches": ["<all_urls>"],
      "js": ["Content.js","utilities/jquery.js"]
    }
  ],
  "web_accessible_resources": [
    {
        "resources": ["img/*"],
        "matches": ["<all_urls>"]
    }
]
}
